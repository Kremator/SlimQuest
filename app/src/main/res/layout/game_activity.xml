        android:layout_below="@+id/b5"
        android:layout_toLeftOf="@+id/b6"
        android:layout_toStartOf="@+id/b6"
        android:src="@drawable/emty" />

    <ImageButton
        android:id="@+id/b10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
